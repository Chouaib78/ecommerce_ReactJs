{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "prodectimg": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "prodecttitle": {
      "type": "string",
      "required": true
    },
    "prodectprice": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "prodectdescription": {
      "type": "text",
      "required": true
    },
    "prodectrating": {
      "type": "decimal",
      "required": true
    },
    "protectcategory": {
      "type": "enumeration",
      "enum": [
        "men",
        "women"
      ],
      "required": true
    }
  }
}
